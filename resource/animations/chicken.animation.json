{
  "format_version": "1.8.0",
  "animations": {
    "animation.morechicken.chicken.baby_transform": {
      "loop": true,
      "bones": {
        "head": {
          "scale": 2.0
        }
      }
    },
    "animation.morechicken.chicken.general": {
      "loop": true,
      "bones": {
        "body": {
          "rotation": ["-this", 0, 0]
        },
        "wing0": {
          "rotation": [0, 0, "math.cos(query.anim_time * 62.8) * 5 + 5"]
        },
        "wing1": {
          "rotation": [0, 0, "math.cos(query.anim_time * 62.8) * -5 - 5"]
        },
        "hat": {
          "rotation": [0, 0, "math.cos(query.anim_time * 62.8) * 3"]
        }
      }
    },
    "animation.morechicken.chicken.walk": {
      "loop": true,
      "anim_time_update": "query.modified_distance_moved",
      "bones": {
        "leg0": {
          "rotation": ["math.cos(query.anim_time * 62.8) * 80.0", 0, 0]
        },
        "leg1": {
          "rotation": ["math.cos(query.anim_time * 62.8) * -80.0", 0, 0]
        },
        "head": {
          "rotation": ["math.cos(query.anim_time * 62.8) *  5", 0, 0]
        },
        "wing0": {
          "rotation": [0, 0, "math.cos(query.anim_time * 62.8) * 45 + 45"]
        },
        "wing1": {
          "rotation": [0, 0, "math.cos(query.anim_time * 62.8) * -45 - 45"]
        },
        "tail": {
          "rotation": ["math.cos(query.anim_time * 62.8) *  60.0 -30", 0, 0]
        },
        "hat": {
          "rotation": [0, 0, "math.cos(query.anim_time * 62.8) * 10"]
        }
      }
    }
  }
}
