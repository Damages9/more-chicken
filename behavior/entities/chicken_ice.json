{
  "format_version": "1.13.0",
  "minecraft:entity": {
    "description": {
      "identifier": "morechicken:chicken_ice",
      "is_spawnable": true,
      "is_summonable": true
    },
    "component_groups": {
      "morechicken:chicken_baby": {
        "minecraft:is_baby": {},
        "minecraft:scale": {
          "value": 0.4
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feed_items": [
            "wheat_seeds",
            "beetroot_seeds",
            "melon_seeds",
            "pumpkin_seeds"
          ],
          "grow_up": {
            "event": "minecraft:ageable_grow_up",
            "target": "self"
          }
        },

        "minecraft:behavior.scared": {
          "priority": 1,
          "sound_interval": 20
        },
        "minecraft:behavior.follow_parent": {
          "priority": 5,
          "speed_multiplier": 1.1
        }
      },
      "morechicken:chicken_angry": {
        "minecraft:angry": {
          "calm_event": {
            "event": "morechicken:on_calm",
            "target": "self"
          }
        }
      },
      "morechicken:chicken_adult": {
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(2,5)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:rideable": {
          "family_types": ["skeleton"],
          "seat_count": 1,
          "seats": {
            "position": [0.0, 0.4, 0.0]
          }
        },
        "minecraft:behavior.breed": {
          "priority": 2,
          "speed_multiplier": 1.0
        },
        "minecraft:loot": {
          "table": "minecraft:loot_tables/entities/chicken.json"
        }
      }
    },
    "components": {
      "minecraft:spawn_entity": {
        "spawn_item": "ice"
      },
      "minecraft:type_family": {
        "family": ["chicken", "mob"]
      },
      "minecraft:attack": {
        "damage": 2.0
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 0
      },
      "minecraft:behavior.melee_attack": {
        "priority": 1,
        "reach_multiplier": 1.0,
        "track_target": true
      },
      "minecraft:behavior.mount_pathing": {
        "priority": 4,
        "speed_multiplier": 1.5,
        "target_dist": 0.0,
        "track_target": true
      },
      "minecraft:behavior.tempt": {
        "can_get_scared": true,
        "can_tempt_vertically": true,
        "items": [
          "wheat_seeds",
          "beetroot_seeds",
          "melon_seeds",
          "pumpkin_seeds"
        ],
        "priority": 5,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.random_stroll": {
        "priority": 6,
        "speed_multiplier": 0.8
      },
      "minecraft:behavior.look_at_player": {
        "priority": 7,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:behavior.random_look_around": {
        "priority": 8
      },
      "minecraft:breathable": {
        "suffocateTime": 0,
        "totalSupply": 6
      },
      "minecraft:breedable": {
        "breed_items": [
          "wheat_seeds",
          "beetroot_seeds",
          "melon_seeds",
          "pumpkin_seeds"
        ],
        "breeds_with": [
          {
            "mate_type": "morechicken:chicken_sand",
            "baby_type": "morechicken:chicken_prismarine",
            "breed_event": {
              "event": "minecraft:entity_born",
              "target": "baby"
            }
          },
          {
            "mate_type": "morechicken:chicken_quartz",
            "baby_type": "morechicken:chicken_glowstone",
            "breed_event": {
              "event": "minecraft:entity_born",
              "target": "baby"
            }
          }
        ],
        "require_tame": false
      },

      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:can_climb": {},
      "minecraft:collision_box": {
        "height": 0.8,
        "width": 0.6
      },
      "minecraft:jump.static": {},
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:movement.basic": {},
      "minecraft:nameable": {},
      "minecraft:navigation.walk": {
        "avoid_damage_blocks": true,
        "avoid_water": true
      },

      "minecraft:health": {
        "value": [2, 4]
      },
      "minecraft:on_target_acquired": {
        "event": "minecraft:become_angry",
        "target": "self"
      },
      "minecraft:on_target_escape": {
        "event": "minecraft:on_calm",
        "target": "self"
      },
      "minecraft:physics": {},
      "minecraft:leashable": {
        "soft_distance": 4.0,
        "hard_distance": 6.0,
        "max_distance": 10.0
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "randomize": [
          {
            "weight": 95,
            "add": {
              "component_groups": ["morechicken:chicken_adult"]
            }
          },
          {
            "weight": 5,
            "add": {
              "component_groups": ["morechicken:chicken_baby"]
            }
          }
        ]
      },
      "morechicken:on_calm": {
        "remove": {
          "component_groups": ["morechicken:chicken_angry"]
        }
      },
      "minecraft:ageable_grow_up": {
        "remove": {
          "component_groups": ["morechicken:chicken_baby"]
        },
        "add": {
          "component_groups": ["morechicken:chicken_adult"]
        }
      },
      "minecraft:entity_born": {
        "add": {
          "component_groups": ["morechicken:chicken_baby"]
        }
      }
    }
  }
}
